<h1>Log In Test</h1>

  <%= form_tag sessions_path do %>

  <div>
    <%= label_tag :email %> <br/>
    <%= text_field_tag :email, params[:email]%>
  </div>

  <div> <br/>
    <%= password_field_tag :password, :placeholder "Password" %>
  </div>


  <div>
    <%= submit_tag "Log in", :class => "button" %>
  </div>
  <% end %>
